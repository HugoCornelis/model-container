		    $$ = IOContainerNewFromIO(NULL);
		}
	|
		TOKEN_BINDABLES
			InputRelations
		TOKEN_END TOKEN_BINDABLES
		{
#line
		    //- remember relations in container on stack

		    $$ = IOContainerNewFromIO($2->pioFirst);
		}
	|
		TOKEN_BINDABLES
			OutputRelations
		TOKEN_END TOKEN_BINDABLES
		{
#line
		    //- remember relations in container on stack

		    $$ = IOContainerNewFromIO($2->pioFirst);
		}
	|
		TOKEN_BINDABLES
			InputRelations
			','
			OutputRelations
		TOKEN_END TOKEN_BINDABLES
		{
#line
		    //- link relations together

		    $2->pioNext = $4;

		    //t loop over output relations and set ->pioFirst ?

		    //- remember relations in container on stack

		    $$ = IOContainerNewFromIO($2->pioFirst);
		}
