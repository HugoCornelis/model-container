#!neurospacesparse
// -*- NEUROSPACES -*-

NEUROSPACES NDF

// VERSION 0.1

IMPORT

	FILE gate1 "gates/km.ndf"

END IMPORT

PRIVATE_MODELS

	ALIAS gate1::/km km_gate END ALIAS

	CHANNEL km
		BINDABLES
			INPUT Vm,OUTPUT Gk,OUTPUT Ik
		END BINDABLES

		PARAMETERS
			PARAMETER ( CHANNEL_TYPE = "ChannelPersistentSteadyStateTau" ),
		END PARAMETERS

		CHILD km_gate km
		END CHILD

	END CHANNEL

END PRIVATE_MODELS

PUBLIC_MODELS

	ALIAS km km
		PARAMETERS
			PARAMETER ( GMAX = 0.400 ),
			PARAMETER ( Ek = -0.085 ),
		END PARAMETERS
	END ALIAS

END PUBLIC_MODELS


