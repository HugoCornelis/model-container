#!neurospacesparse
// -*- NEUROSPACES -*-

NEUROSPACES NDF

// VERSION 0.1

IMPORT

	FILE gate1 "gates/cat_activation.ndf"

	FILE gate2 "gates/cat_inactivation.ndf"

	FILE ca_pool "pools/purkinje_ca.ndf"

END IMPORT

PRIVATE_MODELS

	ALIAS gate1::/cat_activation cat_gate_activation END ALIAS

	ALIAS gate2::/cat_inactivation cat_gate_inactivation END ALIAS

	ALIAS ca_pool::/Ca_concen ca_pool END ALIAS

	CHANNEL cat1
		BINDABLES
			INPUT Vm,OUTPUT G,OUTPUT I
		END BINDABLES

		PARAMETERS
			PARAMETER ( MODEL_SOURCE_TYPE = 0 ),
		END PARAMETERS

		CHILD cat_gate_activation cat_gate_activation
		END CHILD

		CHILD cat_gate_inactivation cat_gate_inactivation
		END CHILD

	END CHANNEL

	CHANNEL cat2
		BINDABLES
			INPUT Vm,OUTPUT G,OUTPUT I
		END BINDABLES

		PARAMETERS
			PARAMETER ( MODEL_SOURCE_TYPE = 0 ),
		END PARAMETERS

		CHILD cat_gate_activation cat_gate_activation
			PARAMETERS
				PARAMETER ( state_init = 0.03891870645 ),
			END PARAMETERS
		END CHILD

		CHILD cat_gate_inactivation cat_gate_inactivation
			PARAMETERS
				PARAMETER ( state_init = 0.08260212813 ),
			END PARAMETERS
		END CHILD

	END CHANNEL

	SEGMENT soma2

		BINDABLES
			OUTPUT Vm
		END BINDABLES

		BINDINGS
			INPUT cat->I
		END BINDINGS

		OPTIONS SPHERICAL

		PARAMETERS
			PARAMETER ( Vm_init = -0.0280 ),
			PARAMETER ( RM = 1.000 ),
			PARAMETER ( RA = 2.50 ),
			PARAMETER ( CM = 0.0164 ),
			PARAMETER ( ELEAK = -0.0800 )
		END PARAMETERS

		CHILD cat1
			cat
			BINDINGS
				INPUT ^->Vm
			END BINDINGS
			PARAMETERS
				PARAMETER ( G_MAX = 5.0 ),
				PARAMETER ( Erev = 0.1375262439 )
			END PARAMETERS
		END CHILD

		CHILD ca_pool
			ca_pool
			BINDINGS
				INPUT ^/cat->I
			END BINDINGS

			PARAMETERS
				PARAMETER
				( BETA = FIXED
					(PARAMETER ( value = 7.579027046e+10 ),
						PARAMETER ( scale = 1.0)  )),
				PARAMETER ( TAU = 0.00010 ),
			END PARAMETERS
		END CHILD

	END SEGMENT

	SEGMENT maind
		BINDABLES
			OUTPUT Vm
		END BINDABLES

		BINDINGS
			INPUT cat->I
		END BINDINGS

		PARAMETERS
			PARAMETER ( Vm_init = -0.0680 ),
			PARAMETER ( RM = 3.0 ),
			PARAMETER ( RA = 2.50 ),
			PARAMETER ( CM = 0.0164 ),
			PARAMETER ( ELEAK = -0.0800 )
		END PARAMETERS

		CHILD cat2
			cat
			BINDINGS
				INPUT ^->Vm
			END BINDINGS
			PARAMETERS
				PARAMETER ( G_MAX = 5.0 ),
				PARAMETER ( Erev = 0.1470214874 )
			END PARAMETERS
		END CHILD

		CHILD ca_pool
			ca_pool
			BINDINGS
				INPUT ^/cat->I
			END BINDINGS

			PARAMETERS
				PARAMETER
				( BETA = FIXED
					(PARAMETER ( value = 9412391936.0 ),
						PARAMETER ( scale = 1.0)  )),
				PARAMETER ( TAU = 0.00020 ),
			END PARAMETERS
		END CHILD

	END SEGMENT

END PRIVATE_MODELS

PUBLIC_MODELS

	CELL doublea_aggregator

		SEGMENT_GROUP segments

			CHILD soma2 c1
				PARAMETERS
					PARAMETER ( rel_X = 0.000e-6 ),
					PARAMETER ( rel_Y = 0.000e-6 ),
					PARAMETER ( rel_Z = 0.000e-6 ),
					PARAMETER ( DIA = 29.80e-6 )
				END PARAMETERS
			END CHILD

			CHILD maind c2
				PARAMETERS
					PARAMETER ( PARENT = ^/c1 ),
					PARAMETER ( rel_X = 5.557e-6 ),
					PARAMETER ( rel_Y = 9.447e-6 ),
					PARAMETER ( rel_Z = 9.447e-6 ),
					PARAMETER ( DIA = 7.72e-6 )
				END PARAMETERS
			END CHILD

		END SEGMENT_GROUP

	END CELL

END PUBLIC_MODELS


