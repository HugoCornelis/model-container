#!neurospacesparse
// -*- NEUROSPACES -*-

NEUROSPACES NDF

IMPORT

	FILE gate1 "gates/kc_activation.ndf"

	FILE gate2 "gates/kc_concentration.ndf"

END IMPORT

PRIVATE_MODELS

	ALIAS gate1::/kc_activation kc_gate_activation END ALIAS

	ALIAS gate2::/kc_concentration kc_gate_concentration END ALIAS

	CHANNEL kc
		BINDABLES
			INPUT Vm,OUTPUT G,OUTPUT I
		END BINDABLES

		PARAMETERS
			PARAMETER ( CHANNEL_TYPE = "ChannelActConc" ),
		END PARAMETERS

		CHILD kc_gate_activation kc_gate_activation
			PARAMETERS
				PARAMETER ( state_init = 0.063531859768213905 ),
			END PARAMETERS
		END CHILD

		CHILD kc_gate_concentration kc_gate_concentration
			PARAMETERS
				PARAMETER ( state_init = 0.0099009900989999993 ),
			END PARAMETERS
		END CHILD

	END CHANNEL

	SEGMENT maind
		BINDABLES
			OUTPUT Vm
		END BINDABLES

		BINDINGS
			INPUT kc->I
		END BINDINGS

		PARAMETERS
			PARAMETER ( Vm_init = -0.0680 ),
			PARAMETER ( RM = 3.0 ),
			PARAMETER ( RA = 2.50 ),
			PARAMETER ( CM = 0.0164 ),
			PARAMETER ( ELEAK = -0.0800 ),

			PARAMETER ( rel_X = 5.557e-6 ),
			PARAMETER ( rel_Y = 9.447e-6 ),
			PARAMETER ( rel_Z = 9.447e-6 ),
			PARAMETER ( DIA = 7.72e-6 ),
		END PARAMETERS

		CHILD kc
			kc
			BINDINGS
				INPUT ^->Vm
			END BINDINGS
			PARAMETERS
				PARAMETER ( GMAX = 800.0 ),
				PARAMETER ( Ek = -0.085 )
			END PARAMETERS
		END CHILD

	END SEGMENT

END PRIVATE_MODELS

PUBLIC_MODELS

	CELL table_kc

		SEGMENT_GROUP segments

			CHILD maind main[0]
			END CHILD

		END SEGMENT_GROUP

	END CELL

END PUBLIC_MODELS


