#!neurospacesparse
// -*- NEUROSPACES -*-

NEUROSPACES NDF

IMPORT

	FILE soma "tests/segments/soma.ndf"

	FILE gate1 "gates/ka_activation.ndf"

	FILE gate2 "gates/ka_inactivation.ndf"

END IMPORT

PRIVATE_MODELS

	ALIAS gate1::/ka_activation ka_gate_activation END ALIAS

	ALIAS gate2::/ka_inactivation ka_gate_inactivation END ALIAS

	CHANNEL ka
		BINDABLES
			INPUT Vm,OUTPUT G,OUTPUT I
		END BINDABLES

		PARAMETERS
			PARAMETER ( CHANNEL_TYPE = "ChannelActInact" ),
		END PARAMETERS

		CHILD ka_gate_activation ka_gate_activation
			PARAMETERS
				PARAMETER ( state_init = 0.08371356085 ),
			END PARAMETERS
		END CHILD

		CHILD ka_gate_inactivation ka_gate_inactivation
			PARAMETERS
				PARAMETER ( state_init = 0.7474853827 ),
			END PARAMETERS
		END CHILD

	END CHANNEL

	SEGMENT soma2

		BINDABLES
			OUTPUT Vm
		END BINDABLES

		BINDINGS
			INPUT ka->I
		END BINDINGS

		OPTIONS SPHERICAL

		PARAMETERS
			PARAMETER ( Vm_init = -0.0680 ),
			PARAMETER ( RM = 1.000 ),
			PARAMETER ( RA = 2.50 ),
			PARAMETER ( CM = 0.0164 ),
			PARAMETER ( ELEAK = -0.0800 )
		END PARAMETERS

		CHILD ka
			ka
			BINDINGS
				INPUT ^->Vm
			END BINDINGS
			PARAMETERS
				PARAMETER ( GMAX = 150.0 ),
				PARAMETER ( Erev = -0.085 )
			END PARAMETERS
		END CHILD

	END SEGMENT

END PRIVATE_MODELS

PUBLIC_MODELS

	CELL table_ka

		SEGMENT_GROUP segments

			CHILD soma2 soma
				PARAMETERS
					PARAMETER ( rel_X = 0.000e-6 ),
					PARAMETER ( rel_Y = 0.000e-6 ),
					PARAMETER ( rel_Z = 0.000e-6 ),
					PARAMETER ( DIA = 29.80e-6 )
				END PARAMETERS
			END CHILD

		END SEGMENT_GROUP

	END CELL

END PUBLIC_MODELS


