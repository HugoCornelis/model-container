#!neurospacesparse
// -*- NEUROSPACES -*-

NEUROSPACES NDF

PRIVATE_MODELS

	CHANNEL kh
		BINDABLES
			INPUT Vm,OUTPUT G,OUTPUT I
		END BINDABLES

		HH_GATE kh
			BINDABLES
				INPUT Vm,OUTPUT activation
			END BINDABLES
	
			BINDINGS
				INPUT ..->Vm
			END BINDINGS

			PARAMETERS

				//m initial value, commonly forward over backward steady states

				PARAMETER ( state_init = 0.03540844397 ),
				PARAMETER ( POWER = 1.0 ),


				PARAMETER ( HH_TABLE_START = 10 ),
				PARAMETER ( HH_TABLE_END = 20 ),
				PARAMETER ( HH_TABLE_STEP = 30 ),

				PARAMETER ( HH_NUMBER_OF_TABLE_ENTRIES_obsoleted = 10 ),

			END PARAMETERS

			GATE_KINETIC_PART h1

				BINDABLES
					INPUT Vm, OUTPUT a
				END BINDABLES

				BINDINGS
					INPUT ..->Vm
				END BINDINGS

				PARAMETERS

					PARAMETER ( table[0] = 10 ),
					PARAMETER ( table[1] = 11 ),
					PARAMETER ( table[2] = 12 ),
					PARAMETER ( table[3] = 13 ),
					PARAMETER ( table[4] = 14 ),
					PARAMETER ( table[5] = 15 ),
					PARAMETER ( table[6] = 16 ),
					PARAMETER ( table[7] = 17 ),
