#!neurospacesparse
// -*- NEUROSPACES -*-

NEUROSPACES NDF

IMPORT

	FILE gate1 "gates/cap_activation.ndf"

	FILE gate2 "gates/cap_inactivation.ndf"

END IMPORT

PRIVATE_MODELS

	ALIAS gate1::/cap_activation cap_gate_activation END ALIAS

	ALIAS gate2::/cap_inactivation cap_gate_inactivation END ALIAS

	CHANNEL cap
		BINDABLES
			INPUT Vm,OUTPUT G,OUTPUT Ik
		END BINDABLES

		PARAMETERS
			PARAMETER ( CHANNEL_TYPE = "ChannelActInact" ),
		END PARAMETERS

		CHILD cap_gate_activation cap_gate_activation
		END CHILD

		CHILD cap_gate_inactivation cap_gate_inactivation
		END CHILD

	END CHANNEL

	SEGMENT maind
		BINDABLES
			OUTPUT Vm
		END BINDABLES

		BINDINGS
			INPUT cap->Ik
		END BINDINGS

		PARAMETERS
			PARAMETER ( Vm_init = -0.0680 ),
			PARAMETER ( RM = 3.0 ),
			PARAMETER ( RA = 2.50 ),
			PARAMETER ( CM = 0.0164 ),
			PARAMETER ( ELEAK = -0.0800 ),

			PARAMETER ( rel_X = 5.557e-6 ),
			PARAMETER ( rel_Y = 9.447e-6 ),
			PARAMETER ( rel_Z = 9.447e-6 ),
			PARAMETER ( DIA = 7.72e-6 ),
		END PARAMETERS

		CHILD cap
			cap
			BINDINGS
				INPUT ^->Vm
			END BINDINGS
			PARAMETERS
				PARAMETER ( GMAX = 45.0 ),
				PARAMETER ( Ek = 0.14702148735523224 )
			END PARAMETERS
		END CHILD

	END SEGMENT

END PRIVATE_MODELS

PUBLIC_MODELS

	CELL table_cap

		SEGMENT_GROUP segments

			CHILD maind main[0]
			END CHILD

		END SEGMENT_GROUP

	END CELL

END PUBLIC_MODELS


