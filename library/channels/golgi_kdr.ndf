#!neurospacesparse
// -*- NEUROSPACES -*-
// Morphology file for Golgi cell
//   http://www.bbf.uia.ac.be/


NEUROSPACES NDF

// VERSION 0.1

// UNITS seconds meters voltage siemens

PUBLIC_MODELS

	/* Delayed Rectifier K current */

	CHANNEL KDr
		BINDABLES
			INPUT Vm,OUTPUT G,OUTPUT Ik
		END BINDABLES

		CHANTABLEFILE "Golgi_kdr37.tab"

		PARAMETERS
			PARAMETER ( Xpower = 4.0 ),
			PARAMETER ( Ypower = 1.0 ),
			PARAMETER ( Zpower = 0.0 )
		END PARAMETERS

	END CHANNEL

END PUBLIC_MODELS


