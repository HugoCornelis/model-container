#!neurospacesparse
// -*- NEUROSPACES -*-

NEUROSPACES NDF

// VERSION 0.1

// UNITS seconds meters voltage siemens

PUBLIC_MODELS

	ATTACHMENT Synapse
		BINDABLES
			INPUT spike,
			OUTPUT activation
		END BINDABLES

		RECEIVES EVENTS(spike)

		ATTRIBUTES weight, delay END ATTRIBUTES

	END ATTACHMENT

END PUBLIC_MODELS


