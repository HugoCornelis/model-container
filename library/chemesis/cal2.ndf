#!neurospacesparse
// -*- NEUROSPACES -*-
NEUROSPACES NDF
// VERSION 0.1
PRIVATE_MODELS
	KINETICS cal1
		PARAMETERS
			PARAMETER ( DIA = 24e-4 ),
			PARAMETER ( LENGTH = 24e-4 ),
		END PARAMETERS

		POOL Ca
			BINDABLES
				OUTPUT concen
			END BINDABLES
			PARAMETERS
				PARAMETER ( "species" = ../../../Ca ),

				PARAMETER ( concen_init = 0.1e-3 ),
// 				PARAMETER ( VAL = 2.0 ),
				PARAMETER ( DIA =  ..->DIA ),
				PARAMETER ( LENGTH =  ..->LENGTH ),
// 				PARAMETER ( THICK = 6.0e-7 ),
				PARAMETER ( "UNITS" = 1e-3 ),
			END PARAMETERS
		END POOL
		POOL Cabuf
			BINDABLES
				OUTPUT concen
			END BINDABLES
			PARAMETERS
				PARAMETER ( concen_init = 0.003 ),
// 				PARAMETER ( VAL = 2.0 ),
				PARAMETER ( DIA =  ..->DIA ),
				PARAMETER ( LENGTH =  ..->LENGTH ),
// 				PARAMETER ( THICK = 6.0e-7 ),
				PARAMETER ( "UNITS" = 1e-3 ),
			END PARAMETERS
		END POOL
		POOL buf
			BINDABLES
				OUTPUT concen
			END BINDABLES
			BINDINGS
				INPUT ../Cabuf->concen,
			END BINDINGS
			PARAMETERS
				PARAMETER ( concen_init = 0.153 ),
				PARAMETER ( concen_total = 0.153 ),
// 				PARAMETER ( VAL = 2.0 ),
				PARAMETER ( DIA =  ..->DIA ),
				PARAMETER ( LENGTH =  ..->LENGTH ),
// 				PARAMETER ( THICK = 6.0e-7 ),
// 				PARAMETER ( "UNITS" = 1e-3 ),
			END PARAMETERS
		END POOL
		REACTION cabufrxn
			BINDABLES
				INPUT concen, OUTPUT concen
			END BINDABLES
			BINDINGS
                		INPUT ("substrate") ../Ca->concen,
				INPUT ("substrate") ../Cabuf->concen,
				INPUT ("product") ../buf->concen,
			END BINDINGS
			PARAMETERS
				PARAMETER ( FORWARD_RATE = 1e2 ),
				PARAMETER ( BACKWARD_RATE = 0.5 ),
			END PARAMETERS
		END REACTION
	END KINETICS

