PUBLIC_MODELS

	GROUP cal2

		SPECIES Ca
			PARAMETERS
				PARAMETER ( VALENCY = 2 ),
			END PARAMETERS
		END SPECIES

		SEGMENT soma
			PARAMETERS
				PARAMETER ( rel_X = 2e-4 ),
				PARAMETER ( rel_Y = 0.000e-6 ),
				PARAMETER ( rel_Z = 0.000e-6 ),
				PARAMETER ( DIA = 8e-4 ),
			END PARAMETERS

			CHILD cal1 cal1
			END CHILD

		END SEGMENT

		SEGMENT dend
			PARAMETERS
				PARAMETER ( PARENT = ^/soma ),
				PARAMETER ( rel_X = 2e-4 ),
				PARAMETER ( rel_Y = 0e-6 ),
				PARAMETER ( rel_Z = 0e-6 ),
				PARAMETER ( DIA = 4e-4 ),
			END PARAMETERS

			PARAMETERS
				PARAMETER ( species[0] = ../Ca ),
				PARAMETER ( diffusion_constant[0] = 6e-9 ),
//				PARAMETER ( "UNITS" = 1e-3 ),
			END PARAMETERS

			CHILD cal1 cal1
			END CHILD

		END SEGMENT

	END GROUP

END PUBLIC_MODELS


